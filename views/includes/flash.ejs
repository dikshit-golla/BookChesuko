
    <% if(success!="") { %>
        <div class="alert alert-secondary alert-dismissible fade show col-6 offset-3" role="alert">
            <%= success %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <% } %>


    <% if(error!="") { %>
        <div class="alert alert-secondary alert-dismissible fade show col-6 offset-3" role="alert">
            <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <% } %>